{
  "order": 0,
  "index_patterns": [
    "log4j-*"
  ],
  "settings": {
    "index": {
      "refresh_interval": "5s",
      "number_of_shards": "1"
    }
  },
  "mappings": {
    "_doc": {
      "dynamic_templates": [
        {
          "strings_as_keyword": {
            "mapping": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "match_mapping_type": "string"
          }
        }
      ],
      "properties": {
        "@timestamp": {
          "type": "date"
        },
        "class": {
          "type": "text",
          "analyzer": "simple"
        },
        "msg": {
          "type": "text",
          "analyzer": "simple"
        }
      }
    }
  },
  "aliases": {}
}
